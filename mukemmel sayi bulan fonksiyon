def mukemmel(sayi):
    mukemmeller=[]
    bolenler=[]

    for i in range(1, sayi):
        toplam = 0
        bolenler=[]
        for j in range(1,i):
            if (i % j == 0):
                bolenler.append(j)
        for k in bolenler:
            toplam+=k
        if toplam==i:
            mukemmeller.append(i)


    print(mukemmeller)


while True:
    x=int(input("sayi="))
    mukemmel(x)



