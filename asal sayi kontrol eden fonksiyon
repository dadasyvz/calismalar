def asal(sayi):
    if  sayi==1:
        return False

    elif sayi==2:
        return True

    else:
        for i in range(2,sayi):
            if sayi%i==0:
                return False

        return True

while True:
    sayi=int(input("sayi="))

    if(asal(sayi)):
        print(sayi,"asal sayidir")

    else:
        print(sayi,"asal sayi degildir")
